apiVersion: v1
kind: Service
metadata:
  labels:
    alertmanager: main
  name: alertmanager-main
  namespace: monitoring
spec:
  type: NodePort
  ports:
  - name: web
    nodePort: {{ alertmanager_nodeport }}
    port: 9093
    targetPort: web
  selector:
    alertmanager: main
    app: alertmanager
